{
  "name": "cypress2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:booking:chrome": "cypress run --record --group bookingPage -s \"cypress/e2e/booking.cy.js\" -b chrome",
    "test:loginAdmin:chrome": "cypress run --record --group adminPage -s \"cypress/e2e/loginAdmin.cy.js\" -b chrome",
    "test:mainPage:chrome": "cypress run --record --group mainPage -s \"cypress/e2e/mainPage.cy.js\" -b chrome",
    "test:booking:firefox": "cypress run --record --group bookingPage -s \"cypress/e2e/booking.cy.js\" -b firefox",
    "test:loginAdmin:firefox": "cypress run --record --group adminPage -s \"cypress/e2e/loginAdmin.cy.js\" -b firefox",
    "test:mainPage:firefox": "cypress run --record --group mainPage -s \"cypress/e2e/mainPage.cy.js\" -b firefox",
    "test:booking:electron": "cypress run --record --group bookingPage -s \"cypress/e2e/booking.cy.js\" -b electron",
    "test:loginAdmin:electron": "cypress run --record --group adminPage -s \"cypress/e2e/loginAdmin.cy.js\" -b electron",
    "test:mainPage:electron": "cypress run --record --group mainPage -s \"cypress/e2e/mainPage.cy.js\" -b electron",
    "test:parallel:chrome": "run-p -c test:booking:chrome test:loginAdmin:chrome test:mainPage:chrome",
    "test:parallel:firefox": "run-p -c test:booking:firefox test:loginAdmin:firefox test:mainPage:firefox",
    "test:parallel:electron": "run-p -c test:booking:electron test:loginAdmin:electron test:mainPage:electron",
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.6.3",
    "npm-run-all": "^4.1.5"
  }
}
